<Window x:Class="EloCalculator.GUI.Main"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EloCalculator.GUI"
        mc:Ignorable="d"
        Title="EloCalculator" Height="450" Width="800">
    <Grid>
        <Menu IsMainMenu="True" Grid.ColumnSpan="3">
            <MenuItem Header="Games" Click="Game_OnClick"/>
            <MenuItem Header="Players" Click="Players_OnClick"/>
            <MenuItem Header="Tournaments" Click="Tournaments_OnClick"/>
        </Menu>
    </Grid>
</Window>
